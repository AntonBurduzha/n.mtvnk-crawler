{
	"name": "buki-crawler",
	"version": "2.0.0",
	"description": "Crawler for https://buki.com.ua/ website to parse profiles of tutors",
	"license": "MIT",
	"author": "Anton Burduzha",
	"type": "commonjs",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/index.js",
		"dev": "ts-node -r tsconfig-paths/register src/index.ts",
		"crawl": "npm run build && npm run start",
		"format": "prettier --write \"src/**/*.ts\""
	},
	"dependencies": {
		"exceljs": "^4.4.0",
		"p-retry": "^6.2.0",
		"pino": "^9.0.0",
		"puppeteer": "^23.0.0",
		"zod": "^3.23.0"
	},
	"devDependencies": {
		"@types/node": "^20.0.0",
		"pino-pretty": "^13.0.0",
		"prettier": "^3.8.1",
		"ts-node": "^10.9.0",
		"tsc-alias": "^1.8.16",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.0"
	}
}
